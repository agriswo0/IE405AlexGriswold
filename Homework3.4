#include <iostream>
#include <cmath>

int digits(int number)
{
    int digits = 0;
    while (number) {
        number /= 10;
        digits++;
    }
    return digits;
}

void word(int number){
    if(number==0){
        std::cout<<"zero ";
    }
    else if(number==1){
        std::cout<<"one ";
    }
    else if(number==2){
        std::cout<<"two ";
    }
    else if(number==3){
        std::cout<<"three ";
    }
    else if(number==4){
        std::cout<<"four ";
    }
    else if(number==5){
        std::cout<<"five ";
    }
    else if(number==6){
        std::cout<<"six ";
    }
    else if(number==7){
        std::cout<<"seven ";
    }
    else if(number==8){
        std::cout<<"eight ";
    }
    else if(number==9){
        std::cout<<"nine ";
    }
}

int main() {
    int val;
    std::cout<<"Enter number: ";
    std::cin>>val;
    
    for(int i=digits(val)-1;i>=0;i--){
        int newVal = val / pow(10,i);
        newVal = newVal % 10;
        word(newVal);
    }
}
