
#include <iostream>
#include <cmath>

int primeFSum(int n) {
    int primes[15] = {};
    int j=0;
    
    while(n%2==0){
        primes[j] = 2;
        n = n/2;
        j++;
    }
    
    for (int i=3; i <= sqrt(n); i = i+2){
        while (n%i == 0){
            primes[j] = i;
            j++;
            n = n/i;
        }
    }
    
    if (n>2){
        primes[j]=n;
    }
    
    int sum = 0;
    for(int i = 0; i<15; i++){
        sum += primes[i];
    }

    return sum;
    
}

bool isPrime(int n){
    //std::cout<<"\n"<<n<<" - ";
    int count = 0;
    for(int i = 1; i <= n; ++i) {
        if(n % i == 0)
            count++;
            //std::cout<<i<<":"<<count<<",";
    }

    if(count>2){
        return false;
    }
    return true;
}

int main(){
    for(int i=2;i<1001;i++){
        int n = primeFSum(i);
        bool iden = isPrime(n);
        if(iden){
            std::cout<<"\nNumber: "<<i<<" - Summed Primes: "<<n;
        }
    }
}
